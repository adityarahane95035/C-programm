#include <stdio.h>
#include<conio.h>

int far, celcius, step, upper, lower;
void main()
{
    lower = 0 ;
    upper = 300;
    step = 20;
    far = lower;
    printf("\tFar\tCelcius");
    while(far <= upper)
    {
        celcius = 5*(far-32) / 9 ;
        printf("\n%10d""%11d\n", far, celcius);
        far = far + step;
    }
}
